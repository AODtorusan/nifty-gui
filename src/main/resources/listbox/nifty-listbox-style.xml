<?xml version="1.0" encoding="UTF-8"?>
<nifty-styles>
  <style id="nifty-listbox">
    <attributes />
  </style>
  <style id="nifty-listbox#scrollpanel">
    <attributes focusable="true" />
    <effect overlay="true">
      <onActive name="blendMode" blendMode="multiply" post="true" />
      <onActive name="imageOverlay" filename="blackborder.png" imageMode="resize:1,30,1,1,1,30,1,1,1,30,1,1" post="true" />
      <onActive name="imageOverlay" post="false" filename="textfield/field.png" imageMode="resize:1,30,1,1,1,30,1,25,1,30,1,5" />
    </effect>
  </style>
  <style id="nifty-listbox#bottom-right">
    <attributes width="28px" height="280px" />
  </style>
  <style id="nifty-listbox-item" base="nifty-label">
    <attributes controller="de.lessvoid.nifty.controls.listbox.ListBoxItemController" inputMapping="de.lessvoid.nifty.input.mapping.MenuInputMapping" />
    <interact onClick="listBoxItemClicked()" />
    <effect>
      <onCustom name="imageOverlay" post="false" filename="textfield/field-selected.png" imageMode="resize:1,30,1,1,1,30,1,30,1,30,1,1" neverStopRendering="true" timeType="infinite" />
      <onCustom name="textColor" post="false" color="#ffff" neverStopRendering="true" timeType="infinite" />
    </effect>
  </style>
</nifty-styles>
